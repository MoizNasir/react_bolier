{
  // "extends": "",
  "compilerOptions": {
    "allowJs": false,
    "allowSyntheticDefaultImports": true,
    "moduleResolution":"node",
    "baseUrl": ".",
    "esModuleInterop": false,
    "isolatedModules": true,
    "jsx": "react",
    "lib": [
      "DOM",
      "DOM.Iterable",
      "ESNext"
    ],
    "module": "ESNext",
    "noEmit": true,
    "paths": {
      "~/*": [
        "src/*"
      ]
    },
    "resolveJsonModule": true,
    "skipLibCheck": false,
    "target": "ESNext",
    // "types": [
    //   ""
    // ]
  }
}